generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model SequelizeMeta {
  name String @id @db.VarChar(255)
}

model appointments {
  id                                    Int       @id @default(autoincrement())
  date                                  DateTime  @db.Timestamptz(6)
  user_id                               Int?
  provider_id                           Int?
  canceled_at                           DateTime? @db.Timestamptz(6)
  created_at                            DateTime  @db.Timestamptz(6)
  updated_at                            DateTime  @db.Timestamptz(6)
  users_appointments_provider_idTousers users?    @relation("appointments_provider_idTousers", fields: [provider_id], references: [id])
  users_appointments_user_idTousers     users?    @relation("appointments_user_idTousers", fields: [user_id], references: [id])
}

model asset_diagnostic {
  id                               Int                   @id @default(autoincrement())
  serial_number                    String                @db.VarChar(255)
  diagnostic_date                  DateTime              @db.Timestamptz(6)
  user_id                          Int
  active                           Boolean
  company_grade                    String?               @db.VarChar(255)
  certified_grade                  String?               @db.VarChar(255)
  cosmetics_screws                 String?               @db.VarChar(255)
  cosmetics_ac_adapter             String?               @db.VarChar(255)
  cosmetics_stylus                 String?               @db.VarChar(255)
  cosmetics_battery                String?               @db.VarChar(255)
  cosmetics_screen                 String?               @db.VarChar(255)
  cosmetics_covers                 String?               @db.VarChar(255)
  cosmetics_tether                 String?               @db.VarChar(255)
  function_storage                 String?               @db.VarChar(255)
  function_storage_extended        Boolean?
  function_ram                     String?               @db.VarChar(255)
  function_mousepad                String?               @db.VarChar(255)
  function_usb                     String?               @db.VarChar(255)
  function_wlan                    String?               @db.VarChar(255)
  function_bluetooth               String?               @db.VarChar(255)
  function_audio                   String?               @db.VarChar(255)
  function_keyboard                String?               @db.VarChar(255)
  function_battery                 String?               @db.VarChar(255)
  function_optical_drive           String?               @db.VarChar(255)
  function_wwan                    String?               @db.VarChar(255)
  function_gps                     String?               @db.VarChar(255)
  function_camera                  String?               @db.VarChar(255)
  function_fingerprint             String?               @db.VarChar(255)
  function_smartcard               String?               @db.VarChar(255)
  function_barcode_reader          String?               @db.VarChar(255)
  function_cfc2_bios_update        String?               @db.VarChar(255)
  function_drivers_installed       String?               @db.VarChar(255)
  function_operating_system        String?               @db.VarChar(255)
  function_windows_activation      Boolean?
  function_coa_report              String?               @db.VarChar(255)
  function_diagnostic_utility      String?               @db.VarChar(255)
  created_at                       DateTime              @db.Timestamptz(6)
  updated_at                       DateTime              @db.Timestamptz(6)
  cosmetics_visual_appearance      String?               @db.VarChar(255)
  cosmetics_label_condition        String?               @db.VarChar(255)
  cosmetics_dents_scratches        String?               @db.VarChar(255)
  cosmetics_missing_parts          Boolean?
  cosmetics_cleaned                Boolean?
  cosmetics_discoloration          String?               @db.VarChar(255)
  cosmetics_latches_hinges_damaged Boolean?
  cosmetics_packaging              String?               @db.VarChar(255)
  function_recovery_media          String?               @db.VarChar(255)
  function_graphics_card           String?               @db.VarChar(255)
  function_screen                  String?               @db.VarChar(255)
  inventory_items_id               Int?
  result_wiping_report             String?               @db.VarChar(255)
  cosmetics_cracks                 String?               @db.VarChar(255)
  bios_password                    String?               @db.VarChar(255)
  function_keyboard_ports          String?               @db.VarChar(255)
  function_storage_2               String?               @db.VarChar(255)
  function_storage_extended_2      String?               @db.VarChar(255)
  function_ram_2                   String?               @db.VarChar(255)
  function_cpu                     String?               @db.VarChar(255)
  inventory_items                  inventory_items?      @relation(fields: [inventory_items_id], references: [id])
  users                            users                 @relation(fields: [user_id], references: [id], onDelete: SetNull)
  inventory_items_log              inventory_items_log[]

  @@unique([diagnostic_date, serial_number], map: "asset_diagnostic_diagnostic_date_serial_number")
  @@index([serial_number, active], map: "asset_diagnostic_serial_number_active")
}

model branches {
  id              Int               @id @default(autoincrement())
  name            String            @unique @db.VarChar(255)
  address1        String?           @db.VarChar(255)
  city            String?           @db.VarChar(255)
  state           String?           @db.VarChar(255)
  country         String?           @db.VarChar(255)
  zip_code        String?           @db.VarChar(255)
  active          Boolean?
  created_at      DateTime          @db.Timestamptz(6)
  updated_at      DateTime          @db.Timestamptz(6)
  inventory       inventory[]
  inventory_items inventory_items[]
}

model brands {
  id          Int      @id @default(autoincrement())
  name        String   @unique @db.VarChar(255)
  description String?  @db.VarChar(255)
  created_at  DateTime @db.Timestamptz(6)
  updated_at  DateTime @db.Timestamptz(6)
  items       items[]
}

model categories {
  id          Int      @id @default(autoincrement())
  name        String   @unique @db.VarChar(255)
  description String?  @db.VarChar(255)
  created_at  DateTime @db.Timestamptz(6)
  updated_at  DateTime @db.Timestamptz(6)
  items       items[]
}

model conditions {
  id          Int      @id @default(autoincrement())
  name        String   @unique @db.VarChar(255)
  description String?  @db.VarChar(255)
  created_at  DateTime @db.Timestamptz(6)
  updated_at  DateTime @db.Timestamptz(6)
  items       items[]
}

model cosmetic_screen_options {
  id         Int      @id @default(autoincrement())
  name       String   @unique @db.VarChar(255)
  severity   String   @db.VarChar(255)
  active     Boolean
  created_at DateTime @db.Timestamptz(6)
  updated_at DateTime @db.Timestamptz(6)
}

model cost_breakdown {
  id                   Int                @id @default(autoincrement())
  cost                 Decimal            @db.Decimal(8, 2)
  inventory_items_id   Int
  description          String             @db.VarChar(255)
  replacement_parts_id Int?
  created_at           DateTime           @db.Timestamptz(6)
  updated_at           DateTime           @db.Timestamptz(6)
  cost_usd             Decimal?           @db.Decimal(8, 2)
  po_currency          String?            @db.VarChar(255)
  inventory_items      inventory_items    @relation(fields: [inventory_items_id], references: [id], onDelete: SetNull)
  replacement_parts    replacement_parts? @relation(fields: [replacement_parts_id], references: [id])
}

model cpus {
  id         Int      @id @default(autoincrement())
  brand      String   @db.VarChar(255)
  number     String   @db.VarChar(255)
  gen        String   @db.VarChar(255)
  speed      String   @db.VarChar(255)
  created_at DateTime @db.Timestamptz(6)
  updated_at DateTime @db.Timestamptz(6)
  items      items[]
}

model customers {
  id                Int      @id @default(autoincrement())
  name              String   @unique @db.VarChar(255)
  phone             String?  @db.VarChar(255)
  email             String?  @db.VarChar(255)
  created_at        DateTime @db.Timestamptz(6)
  updated_at        DateTime @db.Timestamptz(6)
  company           String?  @db.VarChar(255)
  street1           String?  @db.VarChar(255)
  street2           String?  @db.VarChar(255)
  city              String?  @db.VarChar(255)
  postal_code       String?  @db.VarChar(255)
  state             String?  @db.VarChar(255)
  street3           String?  @db.VarChar(255)
  country           String?  @db.VarChar(255)
  residential       Boolean?
  address_verified  String?  @db.VarChar(255)
  customer_username String?  @db.VarChar(255)
  orders            orders[]
}

model departments {
  id         Int      @id @default(autoincrement())
  name       String   @unique @db.VarChar(255)
  created_at DateTime @db.Timestamptz(6)
  updated_at DateTime @db.Timestamptz(6)
}

model downstream {
  id                 Int                  @id @default(autoincrement())
  shipped_date       DateTime?            @db.Timestamptz(6)
  status             String?              @db.VarChar(255)
  file_id            Int?
  supplier_id        Int?
  created_at         DateTime             @db.Timestamptz(6)
  updated_at         DateTime             @db.Timestamptz(6)
  files              files?               @relation(fields: [file_id], references: [id])
  suppliers          suppliers?           @relation(fields: [supplier_id], references: [id])
  downstream_items   downstream_items[]
  downstream_pallets downstream_pallets[]
}

model downstream_items {
  id                 Int              @id @default(autoincrement())
  previous_status    String?          @db.VarChar(255)
  downstream_id      Int?
  user_id            Int
  inventory_items_id Int?
  created_at         DateTime         @db.Timestamptz(6)
  updated_at         DateTime         @db.Timestamptz(6)
  downstream         downstream?      @relation(fields: [downstream_id], references: [id], onDelete: Cascade)
  inventory_items    inventory_items? @relation(fields: [inventory_items_id], references: [id])
  users              users            @relation(fields: [user_id], references: [id], onDelete: SetNull)

  @@unique([downstream_id, inventory_items_id])
}

model downstream_pallets {
  id            Int         @id @default(autoincrement())
  description   String?     @db.VarChar(4000)
  weight        Decimal?    @db.Decimal(10, 3)
  downstream_id Int?
  created_at    DateTime    @db.Timestamptz(6)
  updated_at    DateTime    @db.Timestamptz(6)
  downstream    downstream? @relation(fields: [downstream_id], references: [id], onDelete: Cascade)
}

model exchange_rate_cad_to {
  id         Int      @id @default(autoincrement())
  date       DateTime @db.Timestamptz(6)
  usd        Decimal  @db.Decimal(10, 6)
  mxn        Decimal  @db.Decimal(10, 6)
  created_at DateTime @db.Timestamptz(6)
  updated_at DateTime @db.Timestamptz(6)
}

model files {
  id                                                           Int                  @id @default(autoincrement())
  name                                                         String               @db.VarChar(255)
  path                                                         String?              @unique @db.VarChar(255)
  created_at                                                   DateTime             @db.Timestamptz(6)
  updated_at                                                   DateTime             @db.Timestamptz(6)
  downstream                                                   downstream[]
  inventory_items_inventory_items_battery_file_idTofiles       inventory_items[]    @relation("inventory_items_battery_file_idTofiles")
  inventory_items_inventory_items_wiping_report_file_idTofiles inventory_items[]    @relation("inventory_items_wiping_report_file_idTofiles")
  receiving_pictures                                           receiving_pictures[]
  users                                                        users[]
}

model grading_score_weights {
  id         Int      @id @default(autoincrement())
  status     String   @db.VarChar(255)
  weight     Decimal  @db.Decimal(4, 2)
  created_at DateTime @db.Timestamptz(6)
  updated_at DateTime @db.Timestamptz(6)
}

model instructions {
  id           Int      @id @default(autoincrement())
  name         String   @db.VarChar(255)
  field        String   @db.VarChar(255)
  instructions String?
  created_at   DateTime @db.Timestamptz(6)
  updated_at   DateTime @db.Timestamptz(6)
}

model integration_log {
  id         Int      @id @default(autoincrement())
  service    String?  @db.VarChar(255)
  message    String?  @db.VarChar(6000)
  created_at DateTime @db.Timestamptz(6)
  updated_at DateTime @db.Timestamptz(6)
}

model internal_order_checklists {
  id                      Int                  @id @default(autoincrement())
  internal_order_items_id Int
  internal_order_id       Int
  technician_id           Int
  inventory_items_id      Int
  notes                   String?              @db.VarChar(4000)
  created_at              DateTime             @db.Timestamptz(6)
  updated_at              DateTime             @db.Timestamptz(6)
  internal_orders         internal_orders      @relation(fields: [internal_order_id], references: [id], onDelete: SetNull)
  internal_order_items    internal_order_items @relation(fields: [internal_order_items_id], references: [id], onDelete: SetNull)
  inventory_items         inventory_items      @relation(fields: [inventory_items_id], references: [id], onDelete: SetNull)
  technicians             technicians          @relation(fields: [technician_id], references: [id], onDelete: SetNull)
}

model internal_order_items {
  id                        Int                         @id @default(autoincrement())
  internal_order_id         Int
  quantity                  Int
  title                     String                      @db.VarChar(255)
  notes                     String?                     @db.VarChar(4000)
  created_at                DateTime                    @db.Timestamptz(6)
  updated_at                DateTime                    @db.Timestamptz(6)
  internal_order_checklists internal_order_checklists[]
  internal_orders           internal_orders             @relation(fields: [internal_order_id], references: [id], onDelete: SetNull)
}

model internal_orders {
  id                                           Int                         @id @default(autoincrement())
  internal_order_number                        String                      @unique @db.VarChar(255)
  creation_date                                DateTime                    @db.Timestamptz(6)
  status                                       String                      @db.VarChar(255)
  requested_by_id                              Int
  notes                                        String?                     @db.VarChar(4000)
  created_by_id                                Int
  created_at                                   DateTime                    @db.Timestamptz(6)
  updated_at                                   DateTime                    @db.Timestamptz(6)
  internal_order_checklists                    internal_order_checklists[]
  internal_order_items                         internal_order_items[]
  users_internal_orders_created_by_idTousers   users                       @relation("internal_orders_created_by_idTousers", fields: [created_by_id], references: [id], onDelete: SetNull)
  users_internal_orders_requested_by_idTousers users                       @relation("internal_orders_requested_by_idTousers", fields: [requested_by_id], references: [id], onDelete: SetNull)
}

model inventory {
  id               Int      @id @default(autoincrement())
  branch_id        Int
  item_id          Int
  quantity         Decimal? @db.Decimal
  created_at       DateTime @db.Timestamptz(6)
  updated_at       DateTime @db.Timestamptz(6)
  type             String?  @db.VarChar(255)
  minimum_quantity Int?
  send_email       Boolean?
  bin_quantity     Int?
  branches         branches @relation(fields: [branch_id], references: [id], onDelete: SetNull)
  items            items    @relation(fields: [item_id], references: [id], onDelete: SetNull)

  @@unique([branch_id, item_id])
}

model inventory_assessment {
  id                    Int                 @id @default(autoincrement())
  receiving_item_id     Int
  inventory_items_id    Int
  inventory_item_log_id Int
  created_at            DateTime            @db.Timestamptz(6)
  updated_at            DateTime            @db.Timestamptz(6)
  inventory_items_log   inventory_items_log @relation(fields: [inventory_item_log_id], references: [id], onDelete: SetNull)
  inventory_items       inventory_items     @relation(fields: [inventory_items_id], references: [id], onDelete: SetNull)
  receiving_items       receiving_items     @relation(fields: [receiving_item_id], references: [id], onDelete: SetNull)
}

model inventory_audit {
  id                    Int                     @id @default(autoincrement())
  search_type           String                  @db.VarChar(255)
  search_text           String                  @db.VarChar(255)
  status                String                  @db.VarChar(255)
  user_id               Int
  created_at            DateTime                @db.Timestamptz(6)
  updated_at            DateTime                @db.Timestamptz(6)
  users                 users                   @relation(fields: [user_id], references: [id], onDelete: SetNull)
  inventory_audit_items inventory_audit_items[]
}

model inventory_audit_items {
  id                                                Int              @id @default(autoincrement())
  serial_number                                     String           @db.VarChar(255)
  part_number                                       String?          @db.VarChar(255)
  status                                            String?          @db.VarChar(255)
  action                                            String?          @db.VarChar(255)
  date_action                                       DateTime         @db.Timestamptz(6)
  notes                                             String?          @db.VarChar(9999)
  inventory_audit_id                                Int?
  user_id_create                                    Int
  user_id_action                                    Int?
  created_at                                        DateTime         @db.Timestamptz(6)
  updated_at                                        DateTime         @db.Timestamptz(6)
  inventory_audit                                   inventory_audit? @relation(fields: [inventory_audit_id], references: [id], onDelete: Cascade)
  users_inventory_audit_items_user_id_actionTousers users?           @relation("inventory_audit_items_user_id_actionTousers", fields: [user_id_action], references: [id])
  users_inventory_audit_items_user_id_createTousers users            @relation("inventory_audit_items_user_id_createTousers", fields: [user_id_create], references: [id], onDelete: SetNull)
}

model inventory_history {
  id                 Int              @id @default(autoincrement())
  date               DateTime         @db.Timestamptz(6)
  id_reference       String           @db.VarChar(255)
  route              String           @db.VarChar(255)
  description        String           @db.VarChar(255)
  user_id            Int
  inventory_items_id Int?
  created_at         DateTime         @db.Timestamptz(6)
  updated_at         DateTime         @db.Timestamptz(6)
  notes              String?          @db.VarChar(255)
  inventory_items    inventory_items? @relation(fields: [inventory_items_id], references: [id])
  users              users            @relation(fields: [user_id], references: [id], onDelete: SetNull)
}

model inventory_items {
  id                                                                                                     Int                           @id @default(autoincrement())
  serial_number                                                                                          String                        @unique @db.VarChar(255)
  hours                                                                                                  Decimal?                      @db.Decimal(6, 0)
  grade                                                                                                  String?                       @db.VarChar(255)
  touch                                                                                                  String?                       @db.VarChar(255)
  display                                                                                                String?                       @db.VarChar(255)
  description                                                                                            String?                       @db.VarChar(4000)
  stylus                                                                                                 String?                       @db.VarChar(255)
  wlan                                                                                                   String?                       @db.VarChar(255)
  wwan                                                                                                   String?                       @db.VarChar(255)
  camera                                                                                                 String?                       @db.VarChar(255)
  bluetooth                                                                                              String?                       @db.VarChar(255)
  battery                                                                                                String?                       @db.VarChar(255)
  dvd_drive                                                                                              String?                       @db.VarChar(255)
  gps                                                                                                    String?                       @db.VarChar(255)
  processed_by                                                                                           String?                       @db.VarChar(255)
  condition                                                                                              String?                       @db.VarChar(255)
  storage                                                                                                String?                       @db.VarChar(255)
  ram                                                                                                    String?                       @db.VarChar(255)
  keyboard_type                                                                                          String?                       @db.VarChar(255)
  operating_system                                                                                       String?                       @db.VarChar(255)
  item_id                                                                                                Int
  branch_id                                                                                              Int
  created_at                                                                                             DateTime                      @db.Timestamptz(6)
  updated_at                                                                                             DateTime                      @db.Timestamptz(6)
  status                                                                                                 String?                       @db.VarChar(255)
  cost                                                                                                   String?                       @db.VarChar(255)
  supplier                                                                                               String?                       @db.VarChar(255)
  location                                                                                               String?                       @db.VarChar(255)
  is_processed                                                                                           Boolean?
  is_ready                                                                                               Boolean?
  supplier_id                                                                                            Int?
  ac_adapter                                                                                             String?                       @db.VarChar(255)
  ac_adapter_amp                                                                                         String?                       @db.VarChar(255)
  coa_reported                                                                                           String?                       @db.VarChar(255)
  original_coa                                                                                           String?                       @db.VarChar(255)
  storage_health                                                                                         String?                       @db.VarChar(255)
  usb_ports                                                                                              String?                       @db.VarChar(255)
  windows_activated                                                                                      String?                       @db.VarChar(255)
  wwan_model                                                                                             String?                       @db.VarChar(255)
  cosmetic_screen                                                                                        String?                       @db.VarChar(255)
  storage_serial_number                                                                                  String?                       @db.VarChar(255)
  graphics_card                                                                                          String?                       @db.VarChar(255)
  receiving_item_id                                                                                      Int?
  post_result                                                                                            String?                       @db.VarChar(255)
  received_by                                                                                            Int?
  battery_item_id                                                                                        Int?
  ac_adapter_item_id                                                                                     Int?
  battery_file_id                                                                                        Int?
  wiping_report_file_id                                                                                  Int?
  keyboard_ports                                                                                         String?                       @db.VarChar(255)
  keyboard_type_id                                                                                       Int?
  ram_serial_number                                                                                      String?                       @db.VarChar(255)
  storage_serial_number_2                                                                                String?                       @db.VarChar(255)
