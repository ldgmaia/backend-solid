  ram_2                                                                                                  String?                       @db.VarChar(255)
  storage_health_2                                                                                       String?                       @db.VarChar(255)
  ram_serial_number_2                                                                                    String?                       @db.VarChar(255)
  asset_diagnostic                                                                                       asset_diagnostic[]
  cost_breakdown                                                                                         cost_breakdown[]
  downstream_items                                                                                       downstream_items[]
  internal_order_checklists                                                                              internal_order_checklists[]
  inventory_assessment                                                                                   inventory_assessment[]
  inventory_history                                                                                      inventory_history[]
  items_inventory_items_ac_adapter_item_idToitems                                                        items?                        @relation("inventory_items_ac_adapter_item_idToitems", fields: [ac_adapter_item_id], references: [id])
  files_inventory_items_battery_file_idTofiles                                                           files?                        @relation("inventory_items_battery_file_idTofiles", fields: [battery_file_id], references: [id])
  items_inventory_items_battery_item_idToitems                                                           items?                        @relation("inventory_items_battery_item_idToitems", fields: [battery_item_id], references: [id])
  branches                                                                                               branches                      @relation(fields: [branch_id], references: [id], onDelete: SetNull)
  items_inventory_items_item_idToitems                                                                   items                         @relation("inventory_items_item_idToitems", fields: [item_id], references: [id], onDelete: SetNull)
  users                                                                                                  users?                        @relation(fields: [received_by], references: [id])
  receiving_items                                                                                        receiving_items?              @relation(fields: [receiving_item_id], references: [id])
  suppliers                                                                                              suppliers?                    @relation(fields: [supplier_id], references: [id])
  files_inventory_items_wiping_report_file_idTofiles                                                     files?                        @relation("inventory_items_wiping_report_file_idTofiles", fields: [wiping_report_file_id], references: [id])
  inventory_items_log                                                                                    inventory_items_log[]
  inventory_log                                                                                          inventory_log[]
  order_checklist                                                                                        order_checklist[]
  receiving_items_attachments_receiving_items_attachments_inventory_items_id_attachmentToinventory_items receiving_items_attachments[] @relation("receiving_items_attachments_inventory_items_id_attachmentToinventory_items")
  receiving_items_attachments_receiving_items_attachments_inventory_items_id_deviceToinventory_items     receiving_items_attachments[] @relation("receiving_items_attachments_inventory_items_id_deviceToinventory_items")
  receiving_items_parts                                                                                  receiving_items_parts[]
  replacement_parts                                                                                      replacement_parts[]

  @@index([serial_number], map: "inventory_items_serial_number")
}

model inventory_items_log {
  id                      Int                    @id @default(autoincrement())
  serial_number           String                 @db.VarChar(255)
  hours                   Decimal?               @db.Decimal(6, 0)
  grade                   String?                @db.VarChar(255)
  touch                   String?                @db.VarChar(255)
  display                 String?                @db.VarChar(255)
  description             String?                @db.VarChar(255)
  stylus                  String?                @db.VarChar(255)
  wlan                    String?                @db.VarChar(255)
  wwan                    String?                @db.VarChar(255)
  camera                  String?                @db.VarChar(255)
  bluetooth               String?                @db.VarChar(255)
  battery                 String?                @db.VarChar(255)
